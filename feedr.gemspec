Gem::Specification.new do |s|
  s.name        = 'feedrrr'
  s.version     = '0.0.1'
  s.date        = '2013-02-04'
  s.summary     = "Convert rss feeds to json format"
  s.description = "Made to allow clients to supply an app with a rss feed url and convert it to a json format. To help clients that are unfamiliar with the rss concept the lib allow you to supply it with a number of alternate paths."
  s.authors     = ["Chris Olsen"]
  s.email       = 'olsen.chris@gmail.com'
  s.files       = ["lib/feedrrr.rb"]
  s.homepage    = 'http://github.com/chrisolsen/feedrrr'
end
